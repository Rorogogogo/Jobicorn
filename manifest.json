{
  "manifest_version": 3,
  "name": "Jobicorn",
  "version": "1.0",
  "description": "Open unicorn companies' career pages with filters",
  "permissions": [
    "tabs",
    "sidePanel",
    "storage"
  ],
  "action": {
    "default_title": "Jobicorn"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "side_panel": {
    "default_path": "src/sidebar.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}